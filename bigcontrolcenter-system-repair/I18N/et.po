msgstr "/var/cache/apt/archives kaustad"

msgid "Pasta /tmp"
msgstr "/tmp kaust"

msgid "Pasta .cache de todos os usuarios"
msgstr ".cache kaust kõigi kasutajatele"

msgid "Pasta .thumbnails de todos os usuarios"
msgstr "thumbnails kaust kõigi kasutajatele"

msgid "Cache do Nepomuk/Strigi de todos os usuarios"
msgstr "Kõigi kasutajate Nepomuk/Strig vahemälu"

msgid "Cache do Firefox de todos os usuarios"
msgstr "Kõigi kasutajate Firefoxi vahemälu"

msgid "Arquivos da lixeira de todos os usuarios"
msgstr "Kõigi kasutajate prügifailid"
